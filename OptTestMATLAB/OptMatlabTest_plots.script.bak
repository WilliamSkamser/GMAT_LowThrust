GMAT GroundTrackPlot1.TextureMap = 'ModifiedBlueMarble.jpg';

Create ReportFile TheReport;
GMAT TheReport.SolverIterations = Current;
GMAT TheReport.UpperLeft = [ 0.0988235294117647 0.2063882063882064 ];
GMAT TheReport.Size = [ 0.5852941176470589 0.769041769041769 ];
GMAT TheReport.RelativeZOrder = 52;
GMAT TheReport.Maximized = false;
GMAT TheReport.Filename = 'C:\GMAT_Repo\OptTestMATLAB\DataReport.txt';
GMAT TheReport.Precision = 16;
