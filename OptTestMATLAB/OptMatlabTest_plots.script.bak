GMAT EarthOrbitView.ShowPlot = true;
GMAT EarthOrbitView.MaxPlotPoints = 20000;
GMAT EarthOrbitView.ShowLabels = false;
GMAT EarthOrbitView.ViewPointReference = Earth;
